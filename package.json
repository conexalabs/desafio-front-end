{
  "name": "localizador_empresas",
  "version": "1.0.0",
  "description": "Localizador de Empresas - ConexaLabs 2019",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "jest"
  },
  "author": "Eduardo <eduardojdev@gmail.com>",
  "license": "MIT",
  "homepage": "https://localizador-empresas.herokuapp.com",
  "engines": {
    "node": "~10.15.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
